{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "compressh",
  "version": "0.1.0",
  "identifier": "com.compressh.app",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "fullscreen": false,
        "title": "compressh",
        "width": 600,
        "height": 400
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": ["bin"]
  }
}
